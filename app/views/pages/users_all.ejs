    <div class="container">
        <h1><%= page_title %></h1>

        <% if(users.length == 0) { %>
        <div> Aint got nothing to show ya</div>
        <% } %>
        
        <ul>
			<%	for (var i = 0; i < users.length; i++) {	%>
				<li>
